<p id="notice"><%= notice %></p>

<h1>Bulk Insert Lists</h1>

<%= link_to 'New Import', new_bulk_insert_list_path, id: 'new_import', remote: true %>

<table>
  <thead>
    <tr>
      <th>Created</th>
      <th>ISBNs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bulk_insert_lists.each do |bulk_insert_list| %>
      <tr>
        <td><%= bulk_insert_list.created_at %></td>
        <td><%= bulk_insert_list.EAN13 %></td>
        <td><%= link_to 'Show', bulk_insert_list %></td>
        <td><%= link_to 'Edit', edit_bulk_insert_list_path(bulk_insert_list) %></td>
        <td><%= link_to 'Destroy', bulk_insert_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
